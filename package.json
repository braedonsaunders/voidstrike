    "prepare": "husky"
  },
  "dependencies": {
    "@recast-navigation/three": "^0.43.0",
    "next": "^16.1.2",
    "nostr-tools": "^2.10.4",
    "pako": "^2.1.0",
    "phaser": "^4.0.0-rc.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recast-navigation": "^0.43.0",
    "three": "^0.182.0",
    "zustand": "^5.0.10"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
